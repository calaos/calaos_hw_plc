NO_FLOAT_SCANF	:= 1
NO_FLOAT_PRINTF	:= 1
OPTIMIZATION	:= s
MBED_LIBS	:= fs

NUCLEO_F411RE/core/incconfig.o: $(CONFIG)
NUCLEO_L053R8/core/incconfig.o: $(CONFIG)

include $(GCC4MBED_DIR)/build/gcc4mbed.mk
